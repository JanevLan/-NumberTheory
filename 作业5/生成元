def is_primitive_root(a, p):
    ans = []
    isprime = True
    for i in range(2, p):
        for j in range(2, i):
            if gcd(i, j) == 1:
                continue
            else:
                isprime = False
                break
        if isprime:
            ans.append(i)
        else:
            isprime = True
    for f in ans:
        if pow(a, int((p - 1) / f), p) == 1:
            return False
    return True


p=17
res = []
for i in range(2,p):
    if is_primitive_root(i,p):
       res.append(i)
print(res)
